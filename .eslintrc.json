{
  "env": {
      "browser": true,
      "es2021": true
  },
  "ignorePatterns": "*.spec.js",
  "extends": ["eslint:recommended", "airbnb-base"],
  "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
  },
  "rules": {
      "consistent-return": "off",
      "import/extensions": "off",
      "no-alert": "off",
      "class-methods-use-this": "off",
      "no-underscore-dangle": "off",
      "comma-dangle": "off",
  }
}
